<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
  <Page.actionBar>
    <ActionBar title="nativescript-socketio" icon="" class="action-bar">
    </ActionBar>
  </Page.actionBar>
  <StackLayout height="100%">
    <TextField verticalAlignment="center" hint="Enter username" text="{{ username }}"/>
    <Button verticalAlignment="center" text="Join" tap="{{ join }}" visibility="{{ isSet }}"/>
  </StackLayout>
</Page>
